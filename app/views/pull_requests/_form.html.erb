<%= form_for(pull_request) do |f| %>
  <% if pull_request.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(pull_request.errors.count, "error") %> prohibited this pull_request from being saved:</h2>

      <ul>
      <% pull_request.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :github_id %>
    <%= f.number_field :github_id %>
  </div>

  <div class="field">
    <%= f.label :url %>
    <%= f.text_field :url %>
  </div>

  <div class="field">
    <%= f.label :number %>
    <%= f.number_field :number %>
  </div>

  <div class="field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :body %>
    <%= f.text_field :body %>
  </div>

  <div class="field">
    <%= f.label :assignee %>
    <%= f.text_field :assignee %>
  </div>

  <div class="field">
    <%= f.label :repo_name %>
    <%= f.text_field :repo_name %>
  </div>

  <div class="field">
    <%= f.label :repo_full_name %>
    <%= f.text_field :repo_full_name %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
