<p id="notice"><%= notice %></p>

<h1>Pull Requests</h1>

<table>
  <thead>
    <tr>
      <th>Github</th>
      <th>Url</th>
      <th>Number</th>
      <th>State</th>
      <th>Title</th>
      <th>Body</th>
      <th>Assignee</th>
      <th>Repo name</th>
      <th>Repo full name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pull_requests.each do |pull_request| %>
      <tr>
        <td><%= pull_request.github_id %></td>
        <td><%= pull_request.url %></td>
        <td><%= pull_request.number %></td>
        <td><%= pull_request.state %></td>
        <td><%= pull_request.title %></td>
        <td><%= pull_request.body %></td>
        <td><%= pull_request.assignee %></td>
        <td><%= pull_request.repo_name %></td>
        <td><%= pull_request.repo_full_name %></td>
        <td><%= link_to 'Show', pull_request %></td>
        <td><%= link_to 'Edit', edit_pull_request_path(pull_request) %></td>
        <td><%= link_to 'Destroy', pull_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pull Request', new_pull_request_path %>
